# Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Core Environment
NODE_ENV=development
NEXT_PUBLIC_NODE_ENV=development

# Application URLs
NEXT_PUBLIC_BASE_URL="https://test.example.com"
NEXT_PUBLIC_DEV_BASE_URL="http://localhost:3000"

# Database (Supabase)
# Production
NEXT_PUBLIC_SUPABASE_URL=https://test-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=supabase-key
SUPABASE_SERVICE_ROLE_KEY=supabase-key

# Development
NEXT_PUBLIC_DEV_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_DEV_SUPABASE_ANON_KEY=supabase-key
NEXT_PUBLIC_DEV_SUPABASE_SERVICE_ROLE_KEY=supabase-key

# Payment Processing (Stripe)
STRIPE_SECRET_KEY=sk_live_your-production-key
STRIPE_DEV_SECRET_KEY=sk_test_your-development-key
STRIPE_WEBHOOK_SECRET=whsec_your-production-webhook-secret
STRIPE_WEBHOOK_SECRET_DEV=whsec_your-development-webhook-secret

# External Services

# Google Services
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_PROJECT_ID=your-google-project-id
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour-private-key\n-----END PRIVATE KEY-----\n"
GOOGLE_CLIENT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_SPREADSHEET_ID=your-spreadsheet-id

# Twilio
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token

# Redis
REDIS_URL=redis://localhost:6379

# Voice Services
VOICE_AGENT_SERVICE_URL=https://your-voice-service.com
VOICE_SERVICE_API_KEY=your-voice-service-api-key

# Widget System
WIDGET_JWT_SECRET=your-widget-jwt-secret

# Testing (Optional)
CI=false 